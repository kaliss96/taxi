<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.karen.karenapp.filas_registros">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Placa"
        android:id="@+id/txtPlaca"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="198dp"
        android:layout_marginEnd="198dp"
        android:layout_alignParentStart="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cooperativa"
        android:id="@+id/txtCooperativa"
        android:layout_below="@+id/txtPlaca"
        android:layout_toRightOf="@+id/textView8"
        android:layout_toEndOf="@+id/textView8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre"
        android:id="@+id/txtNombre"
        android:layout_below="@+id/txtCooperativa"
        android:layout_toRightOf="@+id/txtCooperativa"
        android:layout_toEndOf="@+id/txtCooperativa" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Apellido"
        android:id="@+id/txtApellido"
        android:layout_below="@+id/txtNombre"
        android:layout_alignRight="@+id/txtNombre"
        android:layout_alignEnd="@+id/txtNombre" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Telefono"
        android:id="@+id/txtTelefono"
        android:layout_below="@+id/txtApellido"
        android:layout_alignRight="@+id/txtApellido"
        android:layout_alignEnd="@+id/txtApellido" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Marca"
        android:id="@+id/txtMarca"
        android:layout_below="@+id/txtTelefono"
        android:layout_alignRight="@+id/txtTelefono"
        android:layout_alignEnd="@+id/txtTelefono" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Modelo"
        android:id="@+id/txtModelo"
        android:layout_below="@+id/txtMarca"
        android:layout_alignRight="@+id/txtMarca"
        android:layout_alignEnd="@+id/txtMarca" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Color"
        android:id="@+id/txtColor"
        android:layout_below="@+id/txtModelo"
        android:layout_alignRight="@+id/txtModelo"
        android:layout_alignEnd="@+id/txtModelo" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Número Placa:"
        android:id="@+id/textView2"
        android:layout_above="@+id/txtCooperativa"
        android:layout_toLeftOf="@+id/txtCooperativa"
        android:layout_toStartOf="@+id/txtCooperativa"
        android:textColor="@color/accent_material_light"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cooperativa:"
        android:id="@+id/textView3"
        android:textColor="@color/accent_material_light"
        android:textStyle="bold"
        android:layout_below="@+id/textView2"
        android:layout_toLeftOf="@+id/txtCooperativa"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre Conductor:"
        android:id="@+id/textView4"
        android:textColor="@color/accent_material_light"
        android:textStyle="bold"
        android:layout_below="@+id/textView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Apellido Conductor:"
        android:id="@+id/textView5"
        android:textColor="@color/accent_material_light"
        android:textStyle="bold"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView4"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Telefono Cooperativa:"
        android:id="@+id/textView6"
        android:textColor="@color/accent_material_light"
        android:textStyle="bold"
        android:layout_below="@+id/textView5"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Marca Carro:"
        android:id="@+id/textView7"
        android:textColor="@color/accent_material_light"
        android:textStyle="bold"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView6"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Categoría Carro:"
        android:id="@+id/textView8"
        android:textColor="@color/accent_material_light"
        android:textStyle="bold"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView7"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Color Carro:"
        android:id="@+id/textView9"
        android:textColor="@color/accent_material_light"
        android:textStyle="bold"
        android:layout_below="@+id/textView8"
        android:layout_alignRight="@+id/textView8"
        android:layout_alignEnd="@+id/textView8" />

</RelativeLayout>
